<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:orientation="vertical">
	<LinearLayout
		android:id="@+id/linear1"
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		android:background="@drawable/bg_002"
		android:orientation="vertical">
		<LinearLayout
			android:id="@+id/header"
			android:layout_width="match_parent"
			android:layout_height="200dp"
			android:layout_margin="8dp"
			android:padding="8dp"
			android:orientation="vertical">
			<LinearLayout
				android:id="@+id/linear35"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:padding="2dp"
				android:gravity="center_vertical"
				android:orientation="horizontal">
				<ImageView
					android:id="@+id/imageview3"
					android:layout_width="20dp"
					android:layout_height="20dp"
					android:src="@drawable/ic_class_grey"
					android:scaleType="fitCenter" />
				<TextView
					android:id="@+id/textview11"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_marginLeft="10dp"
					android:text="SUBJECT INFORMATION"
					android:textSize="14sp"
					android:textStyle="bold"
					android:textColor="#90A4AE" />
				<LinearLayout
					android:id="@+id/linear39"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:gravity="right|center_vertical"
					android:orientation="horizontal">
					<ImageView
						android:id="@+id/sharelink"
						android:layout_width="30dp"
						android:layout_height="30dp"
						android:src="@drawable/ic_share_grey"
						android:scaleType="fitCenter" />
				</LinearLayout>
			</LinearLayout>
			<LinearLayout
				android:id="@+id/linear8"
				android:layout_width="match_parent"
				android:layout_height="1dp"
				android:background="#CFD8DC"
				android:orientation="horizontal" />
			<TextView
				android:id="@+id/textview2"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_marginTop="10dp"
				android:text="SUBJECT:"
				android:textSize="10sp"
				android:textStyle="bold"
				android:textColor="#90A4AE" />
			<TextView
				android:id="@+id/subjectid"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:text="ID-SAMPLE"
				android:textSize="10sp"
				android:textColor="#90A4AE" />
			<TextView
				android:id="@+id/subjectname"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:text="SAMPLE SUBJECT"
				android:textSize="14sp"
				android:textStyle="bold"
				android:textColor="#546E7A" />
			<TextView
				android:id="@+id/textview14"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:text="STUDENT:"
				android:textSize="10sp"
				android:textStyle="bold"
				android:textColor="#90A4AE" />
			<TextView
				android:id="@+id/course"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:text="COURSEYEARANDSECTION"
				android:textSize="14sp"
				android:textStyle="bold"
				android:textColor="#546E7A" />
			<TextView
				android:id="@+id/textview16"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:text="SEMESTER:"
				android:textSize="10sp"
				android:textStyle="bold"
				android:textColor="#90A4AE" />
			<TextView
				android:id="@+id/sem"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:text="SAMPLESEM"
				android:textSize="14sp"
				android:textStyle="bold"
				android:textColor="#546E7A" />
		</LinearLayout>
		<LinearLayout
			android:id="@+id/linear36"
			android:layout_width="match_parent"
			android:layout_height="40dp"
			android:layout_marginLeft="8dp"
			android:layout_marginRight="8dp"
			android:gravity="center_vertical"
			android:orientation="horizontal">
			<ImageView
				android:id="@+id/add"
				android:layout_width="30dp"
				android:layout_height="30dp"
				android:layout_marginLeft="2dp"
				android:src="@drawable/ic_add_circle_outline_grey"
				android:scaleType="fitCenter" />
			<TextView
				android:id="@+id/stdntn"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:text="0 Student/s"
				android:textSize="14sp"
				android:textStyle="bold"
				android:textColor="#78909C" />
		</LinearLayout>
		<LinearLayout
			android:id="@+id/linear38"
			android:layout_width="match_parent"
			android:layout_height="match_parent"
			android:layout_marginLeft="8dp"
			android:layout_marginRight="8dp"
			android:padding="8dp"
			android:orientation="horizontal">
			<ListView
				android:id="@+id/students"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:padding="8dp"
				android:dividerHeight="0dp"
				android:divider="@null"
				android:choiceMode="none" />
		</LinearLayout>
	</LinearLayout>
</LinearLayout>
