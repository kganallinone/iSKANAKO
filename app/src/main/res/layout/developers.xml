				<de.hdodenhof.circleimageview.CircleImageView
					android:id="@+id/circleimageview10"
					android:layout_width="70dp"
					android:layout_height="70dp"
					android:src="@drawable/dev_jayson"
					app:civ_border_width="3dp"
					app:civ_border_color="#008DCD"
					app:civ_circle_background_color="#FFFFFF"
					app:civ_border_overlay="true" />
				<LinearLayout
					android:id="@+id/linear10"
					android:layout_width="match_parent"
					android:layout_height="match_parent"
					android:padding="8dp"
					android:gravity="center_vertical"
					android:orientation="vertical">
					<TextView
						android:id="@+id/textview16"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:text="LEAD RESEACHER &amp; TESTER"
						android:textSize="10sp"
						android:textStyle="bold"
						android:textColor="#90A4AE" />
					<TextView
						android:id="@+id/textview17"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:text="JAYSON B. ALCANTARA"
						android:textSize="16sp"
						android:textStyle="bold"
						android:textColor="#37474F" />
				</LinearLayout>
			</LinearLayout>
			<LinearLayout
				android:id="@+id/dev5"
				android:layout_width="match_parent"
				android:layout_height="100dp"
				android:layout_margin="5dp"
				android:padding="8dp"
				android:gravity="center_vertical"
				android:orientation="horizontal">
				<de.hdodenhof.circleimageview.CircleImageView
					android:id="@+id/circleimageview11"
					android:layout_width="70dp"
					android:layout_height="70dp"
					android:src="@drawable/dev_kendrick"
					app:civ_border_width="3dp"
					app:civ_border_color="#008DCD"
					app:civ_circle_background_color="#FFFFFF"
					app:civ_border_overlay="true" />
				<LinearLayout
					android:id="@+id/linear12"
					android:layout_width="match_parent"
					android:layout_height="match_parent"
					android:padding="8dp"
					android:gravity="center_vertical"
					android:orientation="vertical">
					<TextView
						android:id="@+id/textview18"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:text="PROJECT DESIGNER"
						android:textSize="10sp"
						android:textStyle="bold"
						android:textColor="#90A4AE" />
					<TextView
						android:id="@+id/textview19"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:text="KENDRICK A. PANINGBATAN"
						android:textSize="16sp"
						android:textStyle="bold"
						android:textColor="#37474F" />
				</LinearLayout>
			</LinearLayout>
			<TextView
				android:id="@+id/textview11"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_marginBottom="10dp"
				android:text="©2022 Unexpected Upgrade. All rights Reserved."
				android:textSize="10sp"
				android:textStyle="bold"
				android:textColor="#CFD8DC" />
		</LinearLayout>
	</ScrollView>
