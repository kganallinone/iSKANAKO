<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:orientation="vertical">
	<LinearLayout
		android:id="@+id/linear1"
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		android:padding="8dp"
		android:background="@drawable/bg_002"
		android:gravity="center_horizontal|center_vertical"
		android:orientation="vertical">
		<ImageView
			android:id="@+id/imageview1"
			android:layout_width="100dp"
			android:layout_height="100dp"
			android:layout_marginTop="2dp"
			android:layout_marginBottom="2dp"
			android:src="@drawable/pup_logo"
			android:scaleType="fitCenter" />
		<LinearLayout
			android:id="@+id/web"
			android:layout_width="200dp"
			android:layout_height="50dp"
			android:layout_marginTop="15dp"
			android:layout_marginBottom="15dp"
			android:background="#B71C1C"
			android:gravity="center_vertical"
			android:orientation="horizontal">
			<ImageView
				android:id="@+id/imageview2"
				android:layout_width="20dp"
				android:layout_height="20dp"
				android:layout_marginLeft="10dp"
				android:src="@drawable/ic_language_white"
				android:scaleType="fitCenter" />
			<LinearLayout
				android:id="@+id/linear7"
				android:layout_width="150dp"
				android:layout_height="wrap_content"
				android:gravity="center_horizontal|center_vertical"
				android:orientation="horizontal">
				<TextView
					android:id="@+id/textview3"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:text="WEBSITE"
					android:textSize="12sp"
					android:textColor="#FFFFFF" />
			</LinearLayout>
		</LinearLayout>
		<LinearLayout
			android:id="@+id/as"
			android:layout_width="200dp"
			android:layout_height="50dp"
			android:layout_marginBottom="15dp"
			android:background="#B71C1C"
			android:gravity="center_vertical"
			android:orientation="horizontal">
			<ImageView
				android:id="@+id/imageview5"
				android:layout_width="20dp"
				android:layout_height="20dp"
				android:layout_marginLeft="10dp"
				android:src="@drawable/ic_language_white"
				android:scaleType="fitCenter" />
			<LinearLayout
				android:id="@+id/linear13"
				android:layout_width="150dp"
				android:layout_height="wrap_content"
				android:gravity="center_horizontal|center_vertical"
				android:orientation="horizontal">
				<TextView
					android:id="@+id/textview6"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:text="APPOINTMENT SYSTEM"
					android:textSize="12sp"
					android:textColor="#FFFFFF" />
			</LinearLayout>
		</LinearLayout>
		<LinearLayout
			android:id="@+id/sis"
			android:layout_width="200dp"
			android:layout_height="50dp"
			android:layout_marginBottom="15dp"
			android:background="#B71C1C"
			android:gravity="center_vertical"
			android:orientation="horizontal">
			<ImageView
				android:id="@+id/imageview3"
				android:layout_width="20dp"
				android:layout_height="20dp"
				android:layout_marginLeft="10dp"
				android:src="@drawable/ic_language_white"
				android:scaleType="fitCenter" />
			<LinearLayout
				android:id="@+id/linear9"
				android:layout_width="150dp"
				android:layout_height="wrap_content"
				android:gravity="center_horizontal|center_vertical"
				android:orientation="horizontal">
				<TextView
					android:id="@+id/textview4"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:text="SIS"
					android:textSize="12sp"
					android:textColor="#FFFFFF" />
			</LinearLayout>
		</LinearLayout>
		<LinearLayout
			android:id="@+id/mabinibox"
			android:layout_width="200dp"
			android:layout_height="50dp"
			android:layout_marginBottom="15dp"
			android:background="#B71C1C"
			android:gravity="center_vertical"
			android:orientation="horizontal">
			<ImageView
				android:id="@+id/imageview4"
				android:layout_width="20dp"
				android:layout_height="20dp"
				android:layout_marginLeft="10dp"
				android:src="@drawable/ic_language_white"
				android:scaleType="fitCenter" />
			<LinearLayout
				android:id="@+id/linear11"
				android:layout_width="150dp"
				android:layout_height="wrap_content"
				android:gravity="center_horizontal|center_vertical"
				android:orientation="horizontal">
				<TextView
					android:id="@+id/textview5"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:text="MABINI BOX"
					android:textSize="12sp"
					android:textColor="#FFFFFF" />
			</LinearLayout>
		</LinearLayout>
	</LinearLayout>
</LinearLayout>
